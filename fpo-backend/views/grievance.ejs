<script>
  document.getElementById('normal').addEventListener('click', function(e) {
    e.preventDefault();
    document.body.style.fontSize = '15px'; 
  });

  document.getElementById('increase').addEventListener('click', function(e) {
    e.preventDefault();
    var currentFontSize = parseFloat(window.getComputedStyle(document.body, null).getPropertyValue('font-size'));
    document.body.style.fontSize = (currentFontSize + 2) + 'px';
  });

  document.getElementById('decrease').addEventListener('click', function(e) {
    e.preventDefault();
    var currentFontSize = parseFloat(window.getComputedStyle(document.body, null).getPropertyValue('font-size'));
    document.body.style.fontSize = (currentFontSize - 2) + 'px';
  });
</script>

