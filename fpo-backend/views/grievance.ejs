            <h3 class="title text-center">Grievance Registration Form(ଅଭିଯୋଗ ପଞ୍ଜିକରଣ ଫର୍ମ)</h3>
            <form class="form" role="form" id="grievanceForm" style="margin-top: 30px" method="post"
              enctype="multipart/form-data">
              <input type="hidden" name="csrf_frm_token" value="634ea8546de005a90d4885cad6aa94db506e7d3f">
              <div class="row">
                <div class="col-lg-6 col-12">
                  <fieldset class="fieldset">
                    <legend class="legend">Person Details</legend>
                    <div class="fieldset-content-col">
                      <div class="row">
                        <div class="col-md-12 col-12 form-group">
                          <label for="" class="form-label">Name of the Petitioner(ଅଭିଯୋଗକାରୀଙ୍କ
                            ନାମ)<span class="text-danger">*</span> </label>
                          <input type="text" name="petitioner_name"  ng-model="petitioner_name" class="form-control">
                        </div>

                        <div class="col-md-6 col-12 form-group">
                          <label for="" class="form-label">Email Id(ଇମେଲ ଆଇଡି)</label>
                          <input type="email" name="email"  ng-model="email" class="form-control" value="">
                        </div>
                        <div class="col-md-6 col-12 form-group">
                          <label for="" class="form-label">Mobile No(ମୋବାଇଲ ନ॰)<span
                              class="text-danger">*</span></label>
                          <input type="text" name="mobile_no"  ng-model="mobile_no" class="form-control"
                            onkeypress="return numbersonly(event)" maxlength="10">
                        </div>

                        <div class="col-md-6 col-12 form-group">
                          <label for="" class="form-label">District (ଜିଲ୍ଲା)<span class="text-danger">*</span></label>
                          <select name="district" class="form-control" ng-model="FpoDistrict" ng-change="loadFpoBlocks()"
                            required>
                            <option value="" disabled selected>--Select--</option>
                            <option ng-repeat="i in FpoDistrictList" value="{{i}}">
                              {{i.districtName}}
                            </option>
                          </select>
                        </div>

                        <div class="col-md-6 col-12 form-group">
                          <label for="" class="form-label">Block (ବ୍ଲକ୍)<span class="text-danger">*</span></label>
                          <select name="block" class="form-control" ng-model="FpoBlock" ng-change="loadFpoGP()" required>
                            <option value="">--Select--</option>
                            <option ng-repeat="b in FpoBlockList" value="{{b}}">{{b.blockName}}
                            </option>
                          </select>
                        </div>

                        <div class="col-md-12 col-12 form-group">
                          <label for="" class="form-label">Address Details(ସମ୍ପୂର୍ଣ ଠିକଣା)<span
                              class="text-danger">*</span></label>
                          <textarea name="address"  ng-model="address" rows="3" class="form-control"
                            placeholder="Enter your address"></textarea>
                        </div>

                      </div>
                    </div>
                  </fieldset>
                </div>
                <div class="col-lg-6 col-12">
