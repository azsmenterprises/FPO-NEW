    <%- include('./commons/header.ejs') -%>

    <section>
        <div class="schemeHead">
            <h1>{{schemeDetails.schemename}}</h1>
        </div>
        <div ng-repeat='scheme in schemeDetails.schemeDetails' class="mainCard">
            <p class="subHead">{{scheme.head}}</p>
            <div class="row">
                <div class="col-md-12">
                    <div class="row" ng-if='scheme.elegibilityCret.length>0'>
                        <div class="col-md-6">
                            <h2 class="headColor">Eligibility Criteria</h2>
                            <hr>
                        </div>
                        <div class="col-md-6">
                            <h2 class="headColor">Particulars</h2>
                            <hr>
                        </div>
                    </div>
                    <div class="row" ng-repeat='schemes in scheme.elegibilityCret'>
                        <div class="col-md-6">
                            <h3>{{schemes.pattern}}</h3>
                        </div>
                        <div class="col-md-6">

                            <div class="row" ng-repeat='perticular in schemes.perticular'>
                                <div class="col-md-12 particularHead">
                                    <h4><b><i>{{perticular.head}}</i></b></h4>

                                </div>
                                <div class="col-md-12 particularContent"
                                    ng-repeat='perticulars in perticular.perticulars'>
                                    <h5>{{$index+1}}. &nbsp; {{perticulars}}</h5>
                                </div><br><br><br>
                            </div>
                        </div>
                    </div>
                </div>


            </div>
            <div class="row">
                <div class="col-md-12">
                    <div class="row">
                        <div class="col-md-6">
                            <h2 class="headColor">Pattern of assistance</h2>
                            <hr>
                        </div>
                        <div class="col-md-6">
                            <h2 class="headColor">Particulars</h2>
                            <hr>
                        </div>
                    </div>
                    <div class="row" ng-repeat='schemes in scheme.patternOfAssistance'>
                        <div class="col-md-6">
                            <h3>{{schemes.pattern}}</h3>
                        </div>
                        <div class="col-md-6">

                            <div class="row" ng-repeat='perticular in schemes.perticular'>
                                <div class="col-md-12 particularHead">
                                    <h4><b><i>{{perticular.head}}</i></b></h4>

                                </div>
                                <div class="col-md-12 particularContent"
                                    ng-repeat='perticulars in perticular.perticulars'>
                                    <h5>{{$index+1}}. &nbsp; {{perticulars}}</h5>
                                </div><br><br><br>
                            </div>
                        </div>
                    </div>
                </div>


            </div>
        </div>

    </section>

    <!-- pdfFile-1627465240780.pdf -->
