          borderWidth: 1,
          data: [12, 19, 3, 5, 2, 6]
        }]
      };

      // Get the canvas element
      var ctx = document.getElementById('myBarChart').getContext('2d');

      // Create the bar chart
      var myBarChart = new Chart(ctx, {
        type: 'bar',
        data: barChartData,
        options: {
          responsive: true,
          scales: {
            x: {
              beginAtZero: true
            },
            y: {
              beginAtZero: true
            }
          }
        }
      });
    });
  </script>
