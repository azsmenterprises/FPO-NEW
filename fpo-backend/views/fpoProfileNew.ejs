  <script>
    document.addEventListener('DOMContentLoaded', function () {
      // Sample data for the bar chart
      var barChartData = {
        labels: ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun'],
        datasets: [{
          label: 'Data Set 1',
          backgroundColor: 'rgba(75, 192, 192, 0.2)',
